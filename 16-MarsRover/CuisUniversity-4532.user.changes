	aCommand = $b ifTrue: [ ^ cardinal moveBackward: self ].
	aCommand = $l ifTrue: [ ^ cardinal rotateLeft: self ].
	aCommand = $r ifTrue: [ ^ cardinal rotateRight: self ].
	
	^ self error: self class ErrorNotValidCommand.! !
