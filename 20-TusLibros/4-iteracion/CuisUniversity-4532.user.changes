	[
		book := (catalog keys at: catalogItemIndex).
		
		restInterface sendAddToCartRequestWith: cartID book: book quantity: addQuantity.
		
		cartItems := (restInterface sendListCartRequestWith: cartID) asOrderedCollection. 
